<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Program.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="agregar-producto.png" />
    <None Remove="buscar.png" />
    <None Remove="inventario.png" />
    <None Remove="planificacion.png" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Program.cs" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="agregar-producto.png" />
    <Resource Include="buscar.png" />
    <Resource Include="inventario.png" />
    <Resource Include="planificacion.png" />
  </ItemGroup>

</Project>
